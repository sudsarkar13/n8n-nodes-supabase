{
  "name": "n8n-nodes-supabase",
	"version": "1.1.0",
  "description": "Comprehensive n8n community node for Supabase with database and storage operations",
  "keywords": [
    "n8n-community-node-package",
    "supabase",
    "database",
    "storage",
    "postgresql",
    "n8n",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-supabase",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-supabase.git"
  },
  "engines": {
    "node": ">=16.10",
    "pnpm": ">=7.18"
  },
  "packageManager": "pnpm@8.6.2",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "pnpm build",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SupabaseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Supabase/Supabase.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^29.6.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/nodes",
      "<rootDir>/credentials"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/*.(test|spec).ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ]
  }
}
